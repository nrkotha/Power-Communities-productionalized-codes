.custom-navbar {
  position: relative;
  width: 100%;
}
.custom-navbar #navBarLinks ul {
  z-index: 100;
  margin: 0;
  padding: 0;
  position: relative;
  list-style: none;
}
.custom-navbar #navBarLinks ul li {
  position: relative;
  display: inline;
}
.custom-navbar #navBarLinks ul li a, 
.custom-navbar #navBarLinks ul li span {
  display: block;
  position: relative;
  padding: 18px 10px;
  text-decoration: none;
}
.custom-navbar #navBarLinks ul li ul {
  position: absolute;
  left: -5000px;
  top: 56px;
  opacity: 0;
  display: none;
  -webkit-transition: opacity .3s, visibility 0s .3s, left 0s .3s;
    transition: opacity .3s, visibility 0s .3s, left 0s .3s;
  background-color:#f2f2f2;
  width:270px;
  box-shadow:none;
}
.custom-navbar #navBarLinks ul li:hover > ul {
  display: block;
  left: 0;
  opacity: 1;
   -webkit-transition: opacity .5s;
   transition: opacity .5s;
}
.custom-navbar #navBarLinks ul li ul li {
  display: list-item;
  float: none;
}
.custom-navbar #navBarLinks ul li ul li ul {
  top: 0;
  left: 100%;
}

.custom-navbar #navBarLinks ul ul li:hover > ul {
  left: 100%;
}
.custom-navbar #navBarLinks ul li ul li a {
  padding: 10px;
  margin: 0;
}
.custom-navbar #navBarLinks ul li > a {
  padding-right: 25px;
}

