<script>
$(document).ready(function() {

  // Remove no-js class
  $('html').removeClass('no-js');

  $('#toggleMenu').on('click', function() {

    if ($(this).hasClass('js-open')) {

      $('.maintop_nav #nav > ul > li:not(#toggleMenu)').removeClass('js-showElement');
      $(this).removeClass('js-open');

    } else {

      $('#nav > ul > li:not(#toggleMenu)').addClass('js-showElement');
      $(this).addClass('js-open');

    }

    return false;
  })

  // Add plus mark to li that have a sub menu
  $('.maintop_nav li:has("ul") > a').append('<span class="plusMark">+</span>');

  // sub menu
  // ------------------------

  $('.maintop_nav li').on('mouseover keyup click mouseleave', function(e) {

    if (e.keyCode === 9 | e.type === 'mouseover') {

      // Show sub menu
    
	  e.stopPropagation();
      $(this).nextAll().children('ul').removeClass('js-showElement');
      $(this).nextAll().children('ul').addClass('js-hideElement');     
      $(this).children('ul').removeClass('js-hideElement');
     $(this).children('ul').addClass('js-showElement');
     $('.js-showElement').siblings('a').addClass('active');
     $('.js-hideElement').siblings('a').removeClass('active');   
	 $("#sitemap").css("visibility", "hidden");
     
    }
	
	
	
});
  // When interacting with a li that has a sub menu
  $('.maintop_nav li:has("ul")').on('mouseover keyup keydown click mouseleave', function(e) {

  
    // If either -
    // tabbing into the li that has a sub menu
    // hovering over the li that has a sub menu
    if (e.keyCode === 9 | e.type === 'mouseover') {

      // Show sub menu
     
	  e.stopPropagation();
      $(this).nextAll().children('ul').removeClass('js-showElement');
      $(this).nextAll().children('ul').addClass('js-hideElement');
      $(this).prevAll().children('ul').removeClass('js-showElement');
      $(this).prevAll().children('ul').addClass('js-hideElement');
      $(this).children('ul').removeClass('js-hideElement');
      $(this).children('ul').addClass('js-showElement');
      $('.js-showElement').siblings('a').addClass('active');
      $('.js-hideElement').siblings('a').removeClass('active');   
    }

    // If mouse leaves li that has sub menu
    if (e.type === 'mouseleave') {

      // hide sub menu
	  e.stopPropagation();
      $(this).children('ul').removeClass('js-showElement');
      $(this).children('ul').addClass('js-hideElement');
      $('.js-showElement').siblings('a').addClass('active');
      $('.js-hideElement').siblings('a').removeClass('active'); 
     	  
    }
     
	 
	


  });

  // Tabbing through Levels of sub menu
  // ------------------------

  // If key is pressed while on the last link in a sub menu
  $('.maintop_nav li > ul > li:last-child > a').on('keydown', function(e) {

    // If tabbing out of the last link in a sub menu AND not tabbing into another sub menu
    if ((e.keyCode == 9 ) && $(this).parent('li').children('ul').length == 0) {

      // Close this sub menu
      $(this).parent('li').parent('ul').removeClass('js-showElement');
      $(this).parent('li').parent('ul').addClass('js-hideElement');
	  

      // If tabbing out of a third level sub menu and there are no other links in the parent (level 2) sub menu
      if ($(this).parent('li').parent('ul').parent('li').parent('ul').parent('li').children('ul').length > 0 && $(this).parent('li').parent('ul').parent('li').is(':last-child')) {

        // Close the parent sub menu (level 2) as well
        $(this).parent('li').parent('ul').parent('li').parent('ul').removeClass('js-showElement');
        $(this).parent('li').parent('ul').parent('li').parent('ul').addClass('js-hideElement');
		
		
      }

    }

  })
  
  // main menu js

$('.tp_left_menu li > ul > li:last-child > a').on('keydown', function(e) {
	  
	 
    // If tabbing out of the last link in a sub menu AND not tabbing into another sub menu
    if ((e.keyCode == 9 ) && $(this).parent('li').children('ul').length == 0) {

      // Close this sub menu
	
      $(this).parent('li').parent('ul').removeClass('js-showElement');
      $(this).parent('li').parent('ul').addClass('js-hideElement');
     
     

      // If tabbing out of a third level sub menu and there are no other links in the parent (level 2) sub menu
      if ($(this).parent('li').parent('ul').parent('li').parent('ul').parent('li').children('ul').length > 0 && $(this).parent('li').parent('ul').parent('li').is(':last-child')) {

        // Close the parent sub menu (level 2) as well
	    $(this).parent('li').parent('ul').parent('li').parent('ul').removeClass('js-showElement');
        $(this).parent('li').parent('ul').parent('li').parent('ul').addClass('js-hideElement');
      }

    }

  })
  



})

</script>
