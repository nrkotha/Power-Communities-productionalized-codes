function Custom_Navigator_Create_Footer(val) { 
    var menu_children;
    var i = 0;
    divWrapper = $('#Custom_Navigator_ajax_result');
    divWrapper = $(divWrapper).find("ul").get(1);
    menu_children = $(divWrapper).children();
    
 
    var menu_length;
    var ctg_count = 0;
    var menu_length = menu_children.length;
    var Custom_Navigator_length = parseInt(menu_length * 27.35);
    var dev_len = 45;
    $("#Custom_Navigator_footer").append('<div class="Custom_Navigator_footer_list_tree" id="Custom_Navigator_footer_list_tree"></div>');    
   
    $(menu_children).each(function(idx, elm) {

        if(i%10 == 0){
          ctg_count = ctg_count+1;
          if (parseInt(menu_length/10) == ctg_count) {
            $("#Custom_Navigator_footer_list_tree").append('<div class="Custom_Navigator_footer_grid" style="float: left; border-right: 1px solid #ccc;" id="grid_'+ctg_count+'"><ul style="padding-left: 10px; padding-right: 10px;"  id="Custom_Navigator_footer_ul'+ctg_count+'"></ul></div>');
            widthUpdate(ctg_count);
          }else{
            $("#Custom_Navigator_footer_list_tree").append('<div class="Custom_Navigator_footer_grid" style="float: left; border-right: 1px solid #ccc;" id="grid_'+ctg_count+'"><ul style="padding-left: 10px; padding-right: 10px;"  id="Custom_Navigator_footer_ul'+ctg_count+'"></ul></div>');
            widthUpdate(ctg_count);
          }
        }  

        if(i >= 0 && i < (ctg_count*10)) {
             $("#Custom_Navigator_footer_ul"+ctg_count).append('<li class="lia-list-tree-toggle-node lia-list-tree-closed">'+$(elm).html()+'</li>');
        }     
        
        elm.setAttribute('onclick',"alert('clicked!');");
        
        i++;
    });

    setTimeout(function totalWidth(){
      for (var i = 1; i <= ctg_count; i++) {
        dev_len = dev_len + document.getElementById('grid_'+i).offsetWidth;
      }

      var total = $("#Custom_Navigator_footer_list_tree").width();
      if (total < dev_len) {
        $('#Custom_Navigator_footer_list_tree').css('width', dev_len+'px');
      }          
    }, 300);
    
 
    
    $("#Custom_Navigator_footer").append('</ul></div>');    
 

}

function widthUpdate(ctg_count){
$( "#Custom_Navigator_footer_ul"+ctg_count ).click(function() {
          var width = document.getElementById('Custom_Navigator_footer_ul'+ctg_count).offsetWidth;
          setTimeout(function changeWidth(){
            var width1 = document.getElementById('Custom_Navigator_footer_ul'+ctg_count).offsetWidth;
            var total = $("#Custom_Navigator_footer_list_tree").width();
            var wt = total + (width1 - width);
            $('#Custom_Navigator_footer_list_tree').css('width', wt+'px');
          }, 800);
        });

}
