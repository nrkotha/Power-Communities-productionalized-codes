function Custom_Navigator_getTreeIDs(th,classes){

    classesArray = classes.split(" "); 
    depth=0;
    treeid=0;
 
    for(classItem in classesArray){
        //alert("class" + classesArray[classItem]);
        if((ttmp = classesArray[classItem].match(/lia-js-data-treeNodeId-([0-9]{1,5})/))){
            //alert("matched t"+ttmp[1]);
            treeid = ttmp[1];
        }
        if((dtmp = classesArray[classItem].match(/lia-js-data-treeNodeDepth-([0-9]{1,5})/))){
            //alert("matched d"+dtmp[1]);
            depth = dtmp [1];
        }   
    }    
    return [treeid,depth];
}


// Do a JSON request with the treeid and depth to get the submenu
function Custom_Navigator_getSubMenu(th,treeid,depth){
    url = "/t5/community/page.boardbrowsertaplet.corenodetree.tree:treenodeloadchildren?t:cp=forums/contributions/page&ticket="+ticketId;
    
    //Parameters : 
    // treeNodeId:1107
    // treeNodeDepth:2
    $.getJSON(url,{treeNodeId:treeid,
                   treeNodeDepth:depth,
                   treeAjaxFeedbackId:"ajaxFeedback_13a746a60e6",
                   eventListeners:"",
                   parameterOverrides:"",
                   triggerEvent:"LITHIUM:treeNodeLoadChildren",
                   eventTargetId:"link_13a746a60e6_61"},
              function(data){
                  
       //Response is in response/parameters/0/content
       subMenu = data["response"]["parameters"][0]["content"];
       th.siblings("ul").remove();
       th.parent().append(subMenu).children("ul").css("padding-left","17px");
    });
}


