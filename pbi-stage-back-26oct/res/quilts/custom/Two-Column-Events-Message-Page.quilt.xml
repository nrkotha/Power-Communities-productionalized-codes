<?xml version="1.0" encoding="UTF-8"?>
<quilt layout="Two-Column-Events-Message-Page" nestable="false" disableTopCssClass="false">
  <add to="event-custom-left">
    <component id="common.widget.breadcrumb"/>
  </add>
  <add to="event-custom-right">
    <component id="custom-widget-create-post"/>
  </add>
  <add to="event-side-content">
    <component id="custom.widget.tour-guide" iStyle="forum" info="true"/>
    <component id="custom.widget.announcement-carousal" type="halfBanner"/>
    <component id="solutions.widget.recently-solved-threads"/>
    <component id="kudos.widget.authors-leaderboard"/>
    <component id="tags.widget.tag-cloud"/>
    <component id="custom.widget.members-online"/>
  </add>
  <add to="event-main-content">
    <component id="custom.msg_event.display"/>
    <component id="custom.widget.forum-thread"/>
  </add>
  <add to="event-common-header">
    <component id="quilt:HeaderPage.2017SR"/>
  </add>
  <remove from="main-content" for="HeaderPage.2017SR">
    <component id="common.widget.breadcrumb"/>
  </remove>
</quilt>