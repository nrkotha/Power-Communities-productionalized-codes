<?xml version="1.0" encoding="UTF-8"?>
<quilt layout="one-column" nestable="false" disableTopCssClass="false">
  <add to="common-footer">
    <component id="quilt:Footer"/>
  </add>
  <add to="main-content">
    <component id="editor"/>
    <component id="ThemesGallery.PostPage.Form"/>
    <component id="custom.form-field.report_url" optional="false"/>
    <component id="custom.form-field.category" featuredCategory="Featured" moderatorRole="administrator"/>
  </add>
  <add to="common-header">
    <component id="quilt:HeaderPageNoTitle"/>
  </add>
  <remove from="site-navigation" for="Header">
    <component id="common.widget.site-navigation"/>
  </remove>
</quilt>