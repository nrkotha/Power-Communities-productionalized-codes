    <component id="quilt:Footer"/>
  </add>
  <add to="side-content">
    <component id="tkb.widget.dashboard-taplet"/>
    <component id="rollup-navigation"/>
    <component id="common.widget.announcement"/>
    <component id="labels.widget.labels-list"/>
    <component id="tkb.widget.top-contributors-taplet"/>
  </add>
  <add to="main-content">
    <component id="tkb.widget.search-bar"/>
